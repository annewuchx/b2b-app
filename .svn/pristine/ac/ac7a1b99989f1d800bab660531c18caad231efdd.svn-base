html,
body,
div.mui-content {
  height: 100%;
}

.mui-bar {
  background-color: #0066FF;
}

[data-dpr="1"] .mui-bar {
  height: 44px;
}

[data-dpr="2"] .mui-bar {
  height: 88px;
}

[data-dpr="3"] .mui-bar {
  height: 132px;
}

[data-dpr="1"] .mui-title {
  height: 44px;
  line-height: 44px;
}

[data-dpr="2"] .mui-title {
  height: 88px;
  line-height: 88px;
}

[data-dpr="3"] .mui-title {
  height: 132px;
  line-height: 132px;
}

.mui-bar span,
a {
  color: #fff;
}

[data-dpr="1"] .mui-bar span,
[data-dpr="1"] a {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar span,
[data-dpr="2"] a {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar span,
[data-dpr="3"] a {
  font-size: 48px;
}

.mui-bar h1 {
  color: #fff;
  font-weight: bold;
}

[data-dpr="1"] .mui-bar h1 {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar h1 {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar h1 {
  font-size: 48px;
}

.mui-bar h5 {
  color: #fff;
  font-weight: bold;
}

[data-dpr="1"] .mui-bar h5 {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar h5 {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar h5 {
  font-size: 48px;
}

.yck-content {
  background-color: #fff;
}

.mui-loader {
  position: absolute;
  top: 30%;
  width: 100%;
  height: 60%;
  color: #888;
  text-align: center;
}

[data-dpr="1"] .mui-loader {
  font-size: 14px;
}

[data-dpr="2"] .mui-loader {
  font-size: 28px;
}

[data-dpr="3"] .mui-loader {
  font-size: 42px;
}