
.bill-withdraw-content {
	margin-top: 10px;
	background: #FFFFFF;
	position: relative;
}

.bank-info div.bank-icon {
	float: left;
	height: 100%;
	line-height: 55px;
}

.bank-info {
	height: 55px;
	width: 100%;
	padding: 0px 15px;
	background: #ffffff;
	margin-bottom: 10px;
	position: relative;
	margin-top: 10px;
}

.withdraw-arrow {
	position: absolute;
	color: #0066FF;
	top: 18px;
	right: 4%;
	font-size: 18px;
	/*px*/
}

.bank-detail {
	padding-top: 5px;
	margin-left: 25px;
	width: 77%;
	float: left;
	height: 100%;
}

.bank-name {
	font-size: 14px;
	/*px*/
	color: rgb(51, 51, 51);
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.bank-num {
	font-size: 14px;
	/*px*/
	color: rgb(119, 119, 119);
}

.bank-card {
	font-size: 40px;
	/*px*/
	margin-bottom: 0px;
	color: #0066ff;
}

.bill-left .bill-left-title {
	padding-top: 15px;
	padding-bottom: 15px;
	padding-left: 4%;
	font-size: 14px;
	/*px*/
}

.bill-left-withdraw {
	margin-bottom: 28px;
	margin-left: 4%;
	margin-right: 4%;
	font-size: 40px;
	/*px*/
	height: 40px;
	line-height: 40px;
	color: #333333;
	position: relative;
}

.dispalyMoney {
	display: inline-block;
	height: 40px;
	position: absolute;
	top: 0px;
	left: 40px;
}

.current-bill {
	margin: 0 4%;
	height: 45px;
	line-height: 45px;
	font-size: 15px;
	position: relative;
	/*px*/
}

.current-bill span {
	color: #8f8f94;
}

.bill-withdraw-tip p {
	font-size: 12px;
	/*px*/
	margin-bottom: 0px;
}

.bill-withdraw-tip {
	margin-top: 10px;
	padding-left: 4%;
}

.with-submit {
	margin-top: 50px;
	padding: 0 4%;
}

.withdraw-input {
	margin-bottom: 0px !important;
	width: 84% !important;
	padding: 0 !important;
	border: 0px !important;
	font-size: 35px !important;
	/*px*/
	line-height: 40px !important;
	position: absolute;
	top: -3px;
	left: 40px;
	display: none;
}

.money-withdraw {
	background: #0066ff;
}

.money-withdraw a,
.money-withdraw h1 {
	color: #FFFFFF;
}

.hint_input {
	display: none;
}

.desp-backdrop {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 998;
	background-color: rgba(0, 0, 0, .5);
}

.desp {
	width: 72%;
	height: 145px;
	border-radius: 7px;
	margin: 0 auto;
	background-color: #FFFFFF;
	overflow: hidden;
	text-overflow: ellipsis;
	color: #000000;
}

@media only screen and (max-width:380px) {
	.desp {
		width: 90%;
	}
}

.hintWord {
	height: 100%;
	padding: 20px;
	text-align: center;
}

.hintWord h3 {
	font-size: 20px;
	/*px*/
	margin: 0px;
}

.hintWord p {
	font-size: 15px;
	/*px*/
}

.desp-loader {
	position: absolute;
	top: 30%;
	width: 100%;
	height: 60%;
	color: #888;
	font-size: 14px;
	/*px*/
	text-align: center;
	z-index: 9999;
}

.disscountMoney {
	font-size: 20px;
	/*px*/
	text-align: center;
	margin: 0;
}



.disscountMoney span {
	font-size: 40px;
	/*px*/
}

.mui-backdrop {
	top: 44px;
}

.bank-info:before,.bill-withdraw-content:before,.current-bill:before{
	
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: #CCCCCC;

}
.bank-info:after,.bill-withdraw-content:after {
    position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: #CCCCCC;
}
