#container-trans-pwd.mui-poppicker {
	-webkit-transform: translateY(330px);
}

#container-trans-pwd.mui-poppicker.mui-active {
	-webkit-transform: translateY(0);
}

#container-trans-pwd .mui-poppicker-body {
	height: auto;
}

#container-trans-pwd .mui-grid-view.mui-grid-9 .mui-table-view-cell {
	padding: 0;
	border-color: #D0D0D0;
}

#container-trans-pwd li.numberarea.mui-active {
	background-color: blue;
	color: white;
}

#container-trans-pwd .pwd-title {
	font-size: 16px;
}

#container-trans-pwd input.password {
	margin-bottom: 0;
	padding: 0;
	border: none;
	position: absolute;
}

input.checkcode {
	width: 85px;
	margin-bottom: 0;
	text-align: center;
	padding: 0 10px;
	border: 0;
	height: 41px;
	float: left;
}


/*input.btnGetCheckCode {
	float: right;
	height: 41px;
	border: 0;
}*/

.btnGetCheckCode {
	width: 40%;
	font-size: 16px;
	padding: 10px 0px !important;
	border-radius: 0px;
	border: 0px;
	height: 100%;
}

.hiddenVRKey {
	display: none !important;
}

.mui-table-view-cell>.mui-btn{
	right: 0px;
}
.fogetPassword{
	color: #06f;
	position: absolute;
	right: 4%;
}

/**
			 * 
			 */

#container-trans-pwd .sixDigitPassword {
	width: 100%;
	height: 41px;
	cursor: text;
	background: #fff;
	outline: none;
	position: relative;
	padding: 0;
}

#container-trans-pwd .sixDigitPassword i {
	width: 16.5%;
	height: 100%;
	float: left;
	display: block;
	padding: 13px 0;
	border-left: 1px solid #cccccc;
}

#container-trans-pwd .sixDigitPassword i:first-child {
	border-left: 0;
}

#container-trans-pwd .sixDigitPassword b {
	display: block;
	margin: 5px auto 4px auto;
	width: 7px;
	height: 7px;
	overflow: hidden;
	display: none;
	/*visibility:hidden;*/
	background: #000;
	border-radius: 100%;
}