.btn-check-code {
  width: 29.33% !important;
  font-size: 0.426667rem;
  padding: 0.266667rem 0 !important;
  border-radius: 0;
  border: 0;
  height: 100%;
}

.mui-input-row label {
  width: 31%;
}

.mui-input-row label~input {
  width: 69%;
}

.mui-phone {
  margin-right: 0.266667rem;
  font-size: 0.426667rem;
}

.contact-description {
  padding: 0.4rem 0.266667rem;
}

.contact-description p {
  margin-bottom: 0;
  color: #333333;
}

.lxwm_form_div button {
  font-size: 0.426667rem;
  margin-bottom: 0;
  padding: 0.266667rem 0;
}

.b2b-header {
  background: #06f;
}

.b2b-header a,
.b2b-header h1 {
  color: #FFFFFF;
}

.b2b-header a span {
  font-size: 0.426667rem;
}

.desp-backdrop {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 998;
  background-color: rgba(0, 0, 0, .5);
}

.desp {
  width: 72%;
  height: 3.866667rem;
  border-radius: 0.186667rem;
  margin: 0 auto;
  background-color: #FFFFFF;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #000000;
}

@media only screen and (max-width:320px) {
  .desp {
    width: 85%;
  }
}

.hintWord {
  height: 100%;
  padding: 0.533333rem;
  text-align: center;
}

.hintWord h3 {
  font-size: 0.533333rem;
  margin: 0;
}

.hintWord p {
  font-size: 0.4rem;
}

.desp-loader {
  position: absolute;
  top: 30%;
  width: 100%;
  height: 60%;
  color: #888;
  font-size: 0.373333rem;
  text-align: center;
  z-index: 9999;
}

.mui-navigate-right:after,
.mui-push-right:after {
  color: #0066ff;
}

.btn-confirm {
  margin-top: 0.826667rem;
  padding: 0.4rem 0.4rem;
}

.btn-call {
  margin-top: 0;
  padding: 0 0.4rem;
}

.checkcode-input {
  width: 35% !important;
  float: left !important;
  padding-right: 0 !important;
}

.form-contact {
  margin-top: 0.266667rem;
}