@charset "utf-8";
* {
    margin: 0;
    padding: 0;
}
html,
body {
    width: 100%;
    height: 100%;
    min-width: 320px;
}
img {
    display: block;
}
ul,
li {
    list-style: none;
}
/*头部*/
.mui-bar {
    background-color: rgb(0, 102, 255);
}
header input.h-right {
    float: right;
    background: rgb(0, 102, 255);
}
header .mui-title {
    color: #fff;
}
header .h-left {
    color: #fff;
}
header .h-left span {
    font-size: 16px;
}
.mui-bar .mui-btn-link {
    color: #5590EF;
}
header input[type=button].h-right-active {
    color: #fff;
}