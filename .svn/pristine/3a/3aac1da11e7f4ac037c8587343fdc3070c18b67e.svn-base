@charset "utf-8";

* {
  margin: 0;
  padding: 0;
}

html,
body {
  width: 100%;
  height: 100%;
  min-width: 8.533333rem;
  background: #fff;
}

img {
  display: block;
}

.mui-content {
  background: #fff;
}

/*content*/

.content {
  width: 100%;
  padding-bottom: 0.533333rem;
}

.content .financing-safe {
  width: 100%;
  text-align: center;
  margin-bottom: 2.4rem;
}

.content .financing-safe img {
  padding-top: 2.666667rem;
  padding-bottom: 1.466667rem;
  display: block;
  width: 60%;
  margin: 0 auto;
}

.content .financing-safe p {
  font-size: 0.56rem;
  color: rgb(0, 102, 255);
  font-weight: lighter;
}

.content .financing-btn {
  width: 92%;
  height: 1.2rem;
  color: #fff;
  background: rgb(0, 102, 255);
  text-align: center;
  font-size: 0.373333rem;
  font-weight: bolder;
  line-height: 1.2rem;
  margin: 0 auto;
  border-radius: 0.133333rem;
}

.content .financing-btn:hover {
  cursor: pointer;
}