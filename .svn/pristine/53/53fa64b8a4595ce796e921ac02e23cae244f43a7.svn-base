/**
 * loading
 */

.load {
	width: 160px;
	height: 55px;
	border-radius: 7px;
	position: absolute;
	background-color: rgba(0, 0, 0, .6);
	overflow: hidden;
	text-overflow: ellipsis;
	top: 0;
	left: 0;
	right: 0;
	margin: auto;
}

.mui-loader {
	position: fixed;
	top: 25%;
	width: 100%;
	height: 50%;
	color: #fff;
	font-size: 12px;
	text-align: center;
	z-index: 8888;
}

.load .loaderWord {
	font-size: 14px;
	float: left;
	display: block;
	line-height: 55px;
}

.load .loader,
.load .loader:after {
	border-radius: 15px;
	width: 30px;
	height: 30px;
}

.load .loader {
	margin: 13px 10px 8px 10px;
	font-size: 8px;
	position: relative;
	text-indent: -9999px;
	border-top: 5px solid rgba(255, 255, 255, 0.2);
	border-right: 5px solid rgba(255, 255, 255, 0.2);
	border-bottom: 5px solid rgba(255, 255, 255, 0.2);
	border-left: 5px solid #ffffff;
	-webkit-animation: load 1.3s infinite linear;
	animation: load 1.3s infinite linear;
	float: left;
}

@-webkit-keyframes load {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-webkit-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}