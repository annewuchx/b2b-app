.bill-tag,
.describe_div {
  float: left;
  margin-right: 3.5%;
  height: 100%;
}

[data-dpr="1"] .bill-tag,
[data-dpr="1"] .describe_div {
  font-size: 14px;
}

[data-dpr="2"] .bill-tag,
[data-dpr="2"] .describe_div {
  font-size: 28px;
}

[data-dpr="3"] .bill-tag,
[data-dpr="3"] .describe_div {
  font-size: 42px;
}

.money_div {
  float: left;
  width: 50%;
  text-align: right;
}

.list_div {
  padding-left: 5.8%;
  height: 1.466667rem;
  background: #FFFFFF;
  clear: both;
}

@media only screen and (max-width: 320px) {
  .list_div {
    padding-left: 2%;
  }

  .describe_div {
    width: 50%;
  }

  [data-dpr="1"] .describe_div {
    font-size: 12px;
  }

  [data-dpr="2"] .describe_div {
    font-size: 24px;
  }

  [data-dpr="3"] .describe_div {
    font-size: 36px;
  }

  .money_div {
    width: 45%;
  }

  [data-dpr="1"] .money_div {
    font-size: 12px !important;
  }

  [data-dpr="2"] .money_div {
    font-size: 24px !important;
  }

  [data-dpr="3"] .money_div {
    font-size: 36px !important;
  }
}

.bill-tag-detail {
  line-height: 1.466667rem;
  margin-right: 3.5%;
}

[data-dpr="1"] .bill-tag-detail {
  font-size: 25px !important;
}

[data-dpr="2"] .bill-tag-detail {
  font-size: 50px !important;
}

[data-dpr="3"] .bill-tag-detail {
  font-size: 75px !important;
}

.describe_div {
  padding-top: 0.213333rem;
  width: 35%;
}

.decribe-comment {
  color: rgb(51, 51, 51);
  overflow: hidden;
  display: inline-block;
  height: 0.453333rem;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
}

.decribe-time {
  color: rgb(119, 119, 119);
}

.body_div_header {
  position: relative;
  height: 0.933333rem;
  line-height: 0.933333rem;
  font-size: 0.4rem;
  margin-top: 0.213333rem;
  padding-left: 4%;
}

.money_div {
  line-height: 1.466667rem;
}

[data-dpr="1"] .money_div {
  font-size: 18px;
}

[data-dpr="2"] .money_div {
  font-size: 36px;
}

[data-dpr="3"] .money_div {
  font-size: 54px;
}

.collect .bill-tag-detail,
.collect .money_div {
  color: rgb(255, 121, 22);
}

.pay .bill-tag-detail,
.pay .money_div {
  color: rgb(118, 175, 27);
}

.bill-tag-txt {
  display: inline-block;
  position: absolute;
  top: 0;
  line-height: 1.466667rem;
  width: 0.666667rem;
  text-align: center;
  color: #FFFFFF;
}

[data-dpr="1"] .bill-tag-txt {
  font-size: 12px;
}

[data-dpr="2"] .bill-tag-txt {
  font-size: 24px;
}

[data-dpr="3"] .bill-tag-txt {
  font-size: 36px;
}

.money-list-content {
  position: relative;
  padding-right: 4%;
  height: 100%;
}

.bill-detail-container {
  position: relative;
}

.bill-detail-container .list_div:last-child .money-list-content {
  border-bottom: none;
}

.body_div_header:after,
.bill-detail-container:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 0.026667rem;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #CCCCCC;
}