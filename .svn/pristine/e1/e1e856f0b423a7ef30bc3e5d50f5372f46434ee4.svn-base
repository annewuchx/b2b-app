html,
body {
  padding: 0;
  margin: 0;
  background: #FFFFFF;
  height: 100%;
}

.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

html,
body,
div.mui-content,
div.mui-slider,
div.mui-slider-group,
div.mui-card {
  height: 100%;
}

.mui-slider .mui-slider-group {
  left: 10%;
}

.mui-slider .mui-slider-group .mui-slider-item {
  width: 80%;
  padding: 1.733333rem 0.4rem 1.866667rem 0.4rem;
}

.mui-slider-indicator {
  position: static;
}

.yck-page-content {
  height: 100%;
}

.pic-item {
  width: 100%;
  height: 100%;
  background-position: center;
  background-repeat: no-repeat;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

.yck-page-content div.pic1 {
  background-image: url(../images/pic_investment_1.png);
}

.yck-page-content div.pic2 {
  background-image: url(../images/pic_investment_2.png);
}

.yck-page-content div.pic3 {
  background-image: url(../images/pic_investment_3.png);
}

.yck-page-content h2 {
  width: 100%;
  text-align: center;
}

.yck-page-content p {
  width: 100%;
  text-align: center;
}

div.add-note {
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 0.666667rem;
  color: rgb(204, 204, 204);
}

[data-dpr="1"] div.add-note {
  font-size: 15px;
}

[data-dpr="2"] div.add-note {
  font-size: 30px;
}

[data-dpr="3"] div.add-note {
  font-size: 45px;
}

p.financefont {
  margin-top: 0.533333rem;
  padding: 0 0.693333rem;
  color: #6B98FF;
}

h2.financefonttitle {
  color: #0066FF;
  margin-top: 0.533333rem;
}

[data-dpr="1"] h2.financefonttitle {
  font-size: 22px;
}

[data-dpr="2"] h2.financefonttitle {
  font-size: 44px;
}

[data-dpr="3"] h2.financefonttitle {
  font-size: 66px;
}