/**
 * mui
 */

.mui-btn,
button,
input[type=button],
input[type=reset],
input[type=submit] {
  font-size: 0.373333rem;
}

.mui-table-view-cell>.mui-badge,
.mui-table-view-cell>.mui-btn,
.mui-table-view-cell>.mui-switch,
.mui-table-view-cell>a>.mui-badge,
.mui-table-view-cell>a>.mui-btn,
.mui-table-view-cell>a>.mui-switch {
  right: 0.4rem;
}

.mui-switch-mini {
  width: 1.253333rem;
}

.mui-switch {
  height: 0.8rem;
  border: 0.053333rem solid #ddd;
  border-radius: 0.533333rem;
}

.mui-switch .mui-switch-handle {
  top: -0.026667rem;
  left: -0.026667rem;
  width: 0.746667rem;
  height: 0.746667rem;
  border-radius: 0.426667rem;
  -webkit-box-shadow: 0 0.053333rem 0.133333rem rgba(0, 0, 0, .4);
  box-shadow: 0 0.053333rem 0.133333rem rgba(0, 0, 0, .4);
}

.mui-table-view-cell {
  position: relative;
  overflow: hidden;
  padding: 0.293333rem 0.4rem;
  -webkit-touch-callout: none;
}

.mui-bar .mui-icon {
  font-size: 24px;
  position: relative;
  z-index: 20;
  padding-top: 0.266667rem;
  padding-bottom: 0.266667rem;
}

.mui-table-view:before {
  position: absolute;
  right: 0;
  left: 0;
  height: 0;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #c8c7cc;
  top: -0.026667rem;
}

.mui-switch-blue.mui-active {
  border: 0.053333rem solid #06f;
  background-color: #06f;
}

.mui-table-view:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 0;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #c8c7cc;
}

.mui-popover .mui-table-view .mui-table-view-cell:first-child,
.mui-popover .mui-table-view .mui-table-view-cell:first-child>a:not(.mui-btn) {
  border-top-left-radius: 0.32rem;
  border-top-right-radius: 0.32rem;
}

.mui-table-view-cell>a:not(.mui-btn) {
  position: relative;
  display: block;
  overflow: hidden;
  margin: -0.293333rem -0.4rem;
  padding: inherit;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: inherit;
}

.mui-popover.mui-popover-action .mui-table-view {
  margin: 0.213333rem;
  text-align: center;
  color: #06f;
  border-radius: 0.106667rem;
}

.mui-popover .mui-table-view {
  overflow: auto;
  max-height: 8rem;
  margin-bottom: 0;
  border-radius: 0.186667rem;
  background-color: #f7f7f7;
  background-image: none;
  -webkit-overflow-scrolling: touch;
}

.mui-popover .mui-table-view .mui-table-view-cell:last-child,
.mui-popover .mui-table-view .mui-table-view-cell:last-child>a:not(.mui-btn) {
  border-bottom-right-radius: 0.32rem;
  border-bottom-left-radius: 0.32rem;
}

body {
  font-family: 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', sans-serif;
  font-size: 0.453333rem;
  color: #000;
  -webkit-overflow-scrolling: touch;
}

/**
 * loading
 */

.load {
  width: 4.266667rem;
  height: 1.466667rem;
  border-radius: 0.186667rem;
  position: absolute;
  background-color: rgba(0, 0, 0, .6);
  overflow: hidden;
  text-overflow: ellipsis;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.mui-loader {
  position: fixed;
  top: 25%;
  width: 100%;
  height: 50%;
  color: #fff;
  font-size: 0.32rem;
  text-align: center;
  z-index: 8888;
}

.load .loaderWord {
  font-size: 0.373333rem;
  float: left;
  display: block;
  line-height: 1.466667rem;
}

.load .loader,
.load .loader:after {
  border-radius: 0.4rem;
  width: 0.8rem;
  height: 0.8rem;
}

.load .loader {
  margin: 0.346667rem 0.266667rem 0.213333rem 0.266667rem;
  font-size: 0.213333rem;
  position: relative;
  text-indent: -266.64rem;
  border-top: 0.133333rem solid rgba(255, 255, 255, 0.2);
  border-right: 0.133333rem solid rgba(255, 255, 255, 0.2);
  border-bottom: 0.133333rem solid rgba(255, 255, 255, 0.2);
  border-left: 0.133333rem solid #ffffff;
  -webkit-animation: load 1.3s infinite linear;
  animation: load 1.3s infinite linear;
  float: left;
}

@-webkit-keyframes load {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}