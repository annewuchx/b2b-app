.bank-back {
  background: #0066ff;
}

.bank-back a,
.bank-back h1 {
  color: #FFFFFF;
}

.mui-loader {
  position: absolute;
  top: 30%;
  width: 100%;
  height: 60%;
  color: #888;
  text-align: center;
}

[data-dpr="1"] .mui-loader {
  font-size: 14px;
}

[data-dpr="2"] .mui-loader {
  font-size: 28px;
}

[data-dpr="3"] .mui-loader {
  font-size: 42px;
}

.mui-backdrop {
  top: 1.173333rem;
}

.mui-input-row label {
  width: 24%;
  text-align: right;
}

@media only screen and (max-width: 350px) {
  .mui-input-row label {
    width: 31%;
    text-align: right;
  }
}

@media only screen and (min-width: 350px) and (max-width: 410px) {
  .mui-input-row label {
    width: 28%;
    text-align: right;
  }
}

.tip {
  text-align: center;
  margin-top: 0.4rem;
  margin-bottom: 0.4rem;
  color: red;
  padding: 0 0.4rem;
}

[data-dpr="1"] .tip {
  font-size: 13px;
}

[data-dpr="2"] .tip {
  font-size: 26px;
}

[data-dpr="3"] .tip {
  font-size: 39px;
}

.mui-navigate-right:after,
.mui-push-right:after {
  color: #0066ff;
}

.mui-switch.mui-active {
  border-color: #0066ff;
  background-color: #0066ff;
}

