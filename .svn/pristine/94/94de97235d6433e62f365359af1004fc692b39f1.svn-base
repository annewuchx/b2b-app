.mui-bar {
  background-color: #0066FF;
}

.mui-bar span,
a {
  color: #fff;
}

[data-dpr="1"] .mui-bar span,
[data-dpr="1"] a {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar span,
[data-dpr="2"] a {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar span,
[data-dpr="3"] a {
  font-size: 48px;
}

.mui-bar h1 {
  color: #fff;
  font-weight: bold;
}

[data-dpr="1"] .mui-bar h1 {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar h1 {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar h1 {
  font-size: 48px;
}

.copyright_msg {
  position: fixed;
  bottom: 0.266667rem;
  width: 100%;
  text-align: center;
}

.mui-navigate-right {
  display: block;
  width: 100%;
  margin-left: 0.4rem;
}

.version-img {
  margin-top: 0.8rem;
  padding: 0;
  margin-bottom: 0.613333rem;
  background-color: #F2F2F2;
  text-align: center;
}

.version {
  color: rgb(119, 119, 119);
}

[data-dpr="1"] .version {
  font-size: 14px;
}

[data-dpr="2"] .version {
  font-size: 28px;
}

[data-dpr="3"] .version {
  font-size: 42px;
}

.img {
  width: 2.986667rem;
  height: 2.8rem;
}

.mui-navigate-right:after,
.mui-push-left:after,
.mui-push-right:after {
  font-family: Muiicons;
  line-height: 1;
  /*no*/
  position: absolute;
  top: 50%;
  display: inline-block;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  text-decoration: none;
  color: #7BA3F6;
  -webkit-font-smoothing: antialiased;
}

[data-dpr="1"] .mui-navigate-right:after,
[data-dpr="1"] .mui-push-left:after,
[data-dpr="1"] .mui-push-right:after {
  font-size: 20px;
}

[data-dpr="2"] .mui-navigate-right:after,
[data-dpr="2"] .mui-push-left:after,
[data-dpr="2"] .mui-push-right:after {
  font-size: 40px;
}

[data-dpr="3"] .mui-navigate-right:after,
[data-dpr="3"] .mui-push-left:after,
[data-dpr="3"] .mui-push-right:after {
  font-size: 60px;
}

.copyright {
  line-height: 0.426667rem;
  margin-bottom: 0.4rem;
  color: rgb(119, 119, 119);
}

[data-dpr="1"] .copyright {
  font-size: 12px;
}

[data-dpr="2"] .copyright {
  font-size: 24px;
}

[data-dpr="3"] .copyright {
  font-size: 36px;
}

.mui-navigate-right {
  color: #333333;
}

[data-dpr="1"] .mui-navigate-right {
  font-size: 15px;
}

[data-dpr="2"] .mui-navigate-right {
  font-size: 30px;
}

[data-dpr="3"] .mui-navigate-right {
  font-size: 45px;
}

.yck-item {
  margin-top: 0.266667rem;
}