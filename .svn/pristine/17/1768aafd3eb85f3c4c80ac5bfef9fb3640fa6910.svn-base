.mui-content,
body,
html {
  background-color: #ffffff;
  height: 100%;
}

.btn-check-code {
  position: absolute;
  margin: auto;
  height: 0.933333rem;
  right: 0;
  top: 0;
  bottom: 0;
  border: 0;
}

.mui-input-group {
  padding: 0 0.4rem;
}

.mui-input-group label {
  width: 15%;
  padding: 0;
  color: #06f;
  font-size: 0.533333rem;
  height: 100%;
  line-height: 1.733333rem;
  padding-left: 0.266667rem;
  padding-right: 0.533333rem;
}

.mui-input-row label~input {
  float: left;
}

.mui-input-group:after {
  height: 0;
}

input[type=color],
input[type=date],
input[type=datetime-local],
input[type=datetime],
input[type=email],
input[type=month],
input[type=number],
input[type=password],
input[type=search],
input[type=tel],
input[type=text],
input[type=time],
input[type=url],
input[type=week],
select,
textarea {
  height: 1.733333rem;
}

.mui-input-group .mui-input-row {
  height: 1.733333rem;
  position: relative;
}

.yck-eye {
  width: 20%;
  color: rgb(204, 204, 204);
  display: inline-block;
  height: 100%;
  line-height: 1.733333rem;
  text-align: right;
}

.mui-input-row .mui-btn {
  width: 2.933333rem;
}

.mui-content-padded {
  margin: 1.866667rem 0.4rem;
}

.mui-btn-block {
  padding: 0.266667rem 0;
}

/*header*/

header.mui-bar {
  background: rgba(255, 255, 255, 0);
}

.mui-bar-nav {
  box-shadow: none;
}

.mui-bar-nav a {
  color: #fff;
}

[data-dpr="1"] .mui-bar-nav a span {
  font-size: 16px;
}

[data-dpr="2"] .mui-bar-nav a span {
  font-size: 32px;
}

[data-dpr="3"] .mui-bar-nav a span {
  font-size: 48px;
}

[data-dpr="1"] .mui-input-row label~input,
[data-dpr="1"] .mui-input-row label~select,
[data-dpr="1"] .mui-input-row label~textarea {
  font-size: 15px;
}

[data-dpr="2"] .mui-input-row label~input,
[data-dpr="2"] .mui-input-row label~select,
[data-dpr="2"] .mui-input-row label~textarea {
  font-size: 30px;
}

[data-dpr="3"] .mui-input-row label~input,
[data-dpr="3"] .mui-input-row label~select,
[data-dpr="3"] .mui-input-row label~textarea {
  font-size: 45px;
}

/*mui-content*/

.backImg {
  width: 100%;
  height: 3.84rem;
  background: url(../images/pic_login.png) no-repeat;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
  margin-bottom: 1.333333rem;
}

.pic-logo img {
  position: absolute;
  display: block;
  width: 30%;
  top: 1.466667rem;
  left: 35%;
}

.mui-bar-nav~.mui-content {
  padding-top: 0;
}

.reg-title {
  font-size: 0.453333rem !important;
  color: rgb(0, 102, 225);
  height: 1.333333rem;
  line-height: 1.333333rem;
  text-align: center;
  margin-bottom: 0;
}

.assist {
  text-align: center;
  font-size: 0.373333rem;
  margin-bottom: 0.4rem;
}

.reg-btn {
  height: 1.2rem;
  font-size: 0.48rem !important;
}

.hline {
  box-sizing: border-box;
  padding: 0 0.4rem;
  line-height: 0;
  background-color: rgb(0, 102, 255);
  margin: 0 0.4rem;
  height: 0.026667rem;
}

/**
 * mui
 */

input,
select,
textarea {
  font-size: 0.453333rem;
}

.mui-btn,
button,
input[type=button],
input[type=reset],
input[type=submit] {
  font-size: 0.373333rem;
  border-radius: 0.08rem;
  border-top-left-radius: 0.08rem;
  border-top-right-radius: 0.08rem;
  border-bottom-right-radius: 0.08rem;
  border-bottom-left-radius: 0.08rem;
}

.yckfont {
  font-size: 0.426667rem;
}

#regFrom {
  -webkit-border-radius: 0.133333rem;
  -moz-border-radius: 0.133333rem;
  border-radius: 0.133333rem;
}

.mui-bar .mui-icon {
  font-size: 0.64rem;
  position: relative;
  z-index: 20;
  padding-top: 0.266667rem;
  padding-bottom: 0.266667rem;
}

.mui-input-group:before {
  height: 0;
}

.mui-input-group .mui-input-row:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0.4rem;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #CCCCCC;
}

.mui-input-row .mui-input-clear~.mui-icon-clear,
.mui-input-row .mui-input-password~.mui-icon-eye,
.mui-input-row .mui-input-speech~.mui-icon-speech {
  height: 1.733333rem;
  line-height: 1.733333rem;
  top: 0;
}