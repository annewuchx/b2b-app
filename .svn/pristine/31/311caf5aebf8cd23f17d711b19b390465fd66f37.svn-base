
.withdraw-submit {
	top: 30%;
	position: fixed;
	opacity: 0;
	height: 155px;
	bottom: 0;
	left: 0px;
	width: 100%;
	display: none;
	box-sizing: border-box;
	-webkit-transition: .3s;
	z-index: 999;
	box-shadow: 0px -5px 7px 0px rgba(0, 0, 0, 0.1);
}
.withdraw-submit.active {
	opacity: 1;
	display: block;
}

.withdraw-submit form{
	margin: 0px;
	font-size: 17px;
}

.withdraw-submit .payAmount{
	border-bottom:3px solid #FF5053;
	margin: 0px 10px;
	padding: 10px 0px;
}
.withdraw-submit .payAmount span{
	color: red;
}

.withdraw-submit .mui-input-group{
	width: 100%;
	z-index: 999;
	padding-bottom: 10px;
}
.withdraw-submit .yck-input-checkcode label~input {
	min-width: 60px;
	width: 120px;
	float: left;
	
}
	
