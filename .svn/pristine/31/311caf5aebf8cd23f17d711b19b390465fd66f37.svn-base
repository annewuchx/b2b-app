#pinPassword .number,#pinPassword .number-reset{
	color: white;
  	line-height:40px;
  	height:100%;
  	width:100%;
  	color:#ffffff;
  	background-color:#ededed;
  	font-size:16px;
	background:linear-gradient(to bottom, #9c9c9c 5%, #7c7c7c 100%);
	border:0px solid #566963;
	border-top-left-radius:3px;
	border-bottom-left-radius: 3px;
	border-top-right-radius:3px;
	border-bottom-right-radius:3px;
	box-shadow: inset 0px -1px 1px 1px #c9c9c9;
	text-align:center;
	display:inline-block;
	text-decoration:none;
}