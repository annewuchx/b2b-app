@charset "utf-8";

* {
  margin: 0;
  padding: 0;
}

html,
body {
  width: 100%;
  height: 100%;
  min-width: 8.533333rem;
}

img {
  display: block;
}

ul,
li {
  list-style: none;
}

/*头部*/

.mui-bar {
  background-color: rgb(0, 102, 255);
}

header .h-right {
  display: block;
  height: 100%;
  float: right;
  color: #fff;
}

[data-dpr="1"] header .h-right {
  font-size: 16px;
}

[data-dpr="2"] header .h-right {
  font-size: 32px;
}

[data-dpr="3"] header .h-right {
  font-size: 48px;
}

header .mui-title {
  color: #fff;
}

header .h-left {
  color: #fff;
}

[data-dpr="1"] header .h-left span {
  font-size: 16px;
}

[data-dpr="2"] header .h-left span {
  font-size: 32px;
}

[data-dpr="3"] header .h-left span {
  font-size: 48px;
}

.mui-bar .mui-btn-link {
  color: #fff;
}

.mui-bar {
  padding: 0 0.4rem 0 0.24rem;
}

[data-dpr="1"] .mui-title {
  font-size: 16px;
}

[data-dpr="2"] .mui-title {
  font-size: 32px;
}

[data-dpr="3"] .mui-title {
  font-size: 48px;
}

.headerTop {
  width: 100%;
  height: 0.773333rem;
}

/*content*/

.mui-content {
  padding-bottom: 2.666667rem;
}

/*footer*/

footer {
  height: 1.306667rem;
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 99;
  background: #fff;
}

footer .immediate-financing {
  position: absolute;
  width: 100%;
  height: 1.306667rem;
  text-align: center;
  background: rgb(0, 102, 255);
  font-weight: bold;
  line-height: 1.306667rem;
  color: #fff;
  right: 0;
  top: 0;
}

[data-dpr="1"] footer .immediate-financing {
  font-size: 16px;
}

[data-dpr="2"] footer .immediate-financing {
  font-size: 32px;
}

[data-dpr="3"] footer .immediate-financing {
  font-size: 48px;
}

footer .immediate-financing:hover {
  cursor: pointer;
}

#forget-pas {
  padding-top: 0.133333rem;
}

/*头部*/

.mui-card {
  box-shadow: none;
  background: #efeff4;
  margin: 0;
  border: 0;
  border-radius: 0;
}

.mui-table-view {
  background: #efeff4;
}

.bg-pos p:nth-child(1) {
  height: 6rem;
}

.bg-pos p:nth-child(2) {
  height: 0.933333rem;
}

.mui-table-view-cell {
  padding: 0;
  background: #efeff4;
}

li.bgColor {
  background-color: #fff;
}

.mui-table-view-cell>div.pj-tit {
  padding: 0;
  height: 0.746667rem;
  position: relative;
  background: rgb(255, 122, 21);
  margin-bottom: 0.266667rem;
  margin-top: 0.266667rem;
  width: 100%;
}

.pj-tit ul {
  width: 98%;
  margin-left: 2%;
  height: 100%;
  position: relative;
}

.pj-tit ul li {
  height: 0.4rem;
  line-height: 0.426667rem;
  margin: 0.186667rem 0;
  float: left;
}

.pj-tit ul li:nth-child(1) {
  position: absolute;
  left: -0.186667rem;
  top: -0.186667rem;
  width: 0.106667rem;
  height: 100%;
  z-index: 9;
  background: rgb(255, 204, 0);
}

.pj-tit ul li p {
  color: #fff;
}

[data-dpr="1"] .pj-tit ul li p {
  font-size: 12px;
}

[data-dpr="2"] .pj-tit ul li p {
  font-size: 24px;
}

[data-dpr="3"] .pj-tit ul li p {
  font-size: 36px;
}

.pj-tit .pj-tit-order {
  width: 54%;
  padding-left: 3%;
  border-right: 1px solid #fff;
}

.pj-tit ul li.no {
  position: absolute;
  left: 55.5%;
  color: #fff;
}

[data-dpr="1"] .pj-tit ul li.no {
  font-size: 12px;
}

[data-dpr="2"] .pj-tit ul li.no {
  font-size: 24px;
}

[data-dpr="3"] .pj-tit ul li.no {
  font-size: 36px;
}

.pj-tit .pj-tit-order1 {
  margin-left: 8%;
}

.pj-check {
  z-index: 10;
}

.mui-checkbox input[type=checkbox] {
  top: 0.106667rem;
  left: 0.133333rem;
}

/*融资内容整体部分*/

.pj-con {
  width: 95.2%;
  height: 2.24rem;
  margin: 0.266667rem auto 0.266667rem auto;
  border: 1px solid rgb(204, 204, 204);
  border-top: none;
  background: #fff;
}

.mui-ios .pj-con {
  border: 0.5px solid rgb(204, 204, 204);
}

.mui-checkbox1 input[type=checkbox] {
  top: 0.48rem;
  margin-top: -0.346667rem;
}

/*mui-collapse-content-right*/

.mui-collapse-content-right {
  float: left;
  width: 100%;
  height: 100%;
  color: #646464;
}

/*.con-right-top*/

.con-right-top {
  height: 0.64rem;
}

.con-right-top {
  background: rgb(111, 168, 255);
}

.con-right-top li {
  height: 0.373333rem;
  display: inline-block;
  color: #fff;
  margin: 0.133333rem auto;
  line-height: 0.4rem;
}

[data-dpr="1"] .con-right-top li {
  font-size: 12px;
}

[data-dpr="2"] .con-right-top li {
  font-size: 24px;
}

[data-dpr="3"] .con-right-top li {
  font-size: 36px;
}

.con-right-top li:nth-child(1) {
  width: 60%;
  margin-left: 3.75%;
  border-right: 1px solid #fff;
}

.con-right-top li:nth-child(2) {
  margin-left: 0.4rem;
}

/*.dz-money*/

.dz-money {
  text-align: center;
  height: 1.6rem;
}

.dz-money .mui-checkbox input.bill {
  z-index: 999;
  left: 0.16rem;
  top: 0.8rem;
  margin-top: -0.253333rem;
}

.dz-money ul {
  position: relative;
}

.dz-money ul li a {
  color: rgb(51, 51, 51);
}

[data-dpr="1"] .dz-money ul li a {
  font-size: 12px;
}

[data-dpr="2"] .dz-money ul li a {
  font-size: 24px;
}

[data-dpr="3"] .dz-money ul li a {
  font-size: 36px;
}

.dz-money ul li:nth-child(1) {
  color: rgb(51, 51, 51);
  line-height: 0.746667rem;
}

[data-dpr="1"] .dz-money ul li:nth-child(1) {
  font-size: 12px;
}

[data-dpr="2"] .dz-money ul li:nth-child(1) {
  font-size: 24px;
}

[data-dpr="3"] .dz-money ul li:nth-child(1) {
  font-size: 36px;
}

.dz-money ul li:nth-child(2) {
  color: rgb(255, 180, 0);
}

[data-dpr="1"] .dz-money ul li:nth-child(2) {
  font-size: 18px;
}

[data-dpr="2"] .dz-money ul li:nth-child(2) {
  font-size: 36px;
}

[data-dpr="3"] .dz-money ul li:nth-child(2) {
  font-size: 54px;
}

[data-dpr="1"] .dz-money ul li:nth-child(2) span {
  font-size: 24px;
}

[data-dpr="2"] .dz-money ul li:nth-child(2) span {
  font-size: 48px;
}

[data-dpr="3"] .dz-money ul li:nth-child(2) span {
  font-size: 72px;
}

.dz-money ul li:nth-child(3) {
  position: absolute;
  width: 0.213333rem;
  height: 0.426667rem;
  right: 0.586667rem;
  top: 0.8rem;
  margin-top: -0.213333rem;
  color: rgb(111, 168, 255);
}

/**/

.mui-card .mui-table-view .mui-table-view-cell:first-child,
.mui-card .mui-table-view .mui-table-view-divider:first-child {
  border-radius: 0;
}

/*order-detail*/

.order-detail {
  background: #fff;
  z-index: 44;
  left: 0;
  right: 0;
  position: fixed;
}

[data-dpr="1"] .order-detail {
  top: 54px;
}

[data-dpr="2"] .order-detail {
  top: 108px;
}

[data-dpr="3"] .order-detail {
  top: 162px;
}

.order-detail ul:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  bottom: 0;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #c8c7cc;
}

.order-detail ul li p {
  display: inline-block;
}

[data-dpr="1"] .order-detail ul li p {
  font-size: 15px;
}

[data-dpr="2"] .order-detail ul li p {
  font-size: 30px;
}

[data-dpr="3"] .order-detail ul li p {
  font-size: 45px;
}

.order-detail .money p:nth-child(2) {
  color: rgb(255, 180, 0);
}

.order-detail ul li {
  position: relative;
  height: 1.2rem;
  line-height: 1.2rem;
  width: 96%;
  margin-left: 4%;
}

.order-detail ul li:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  top: 0;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: #c8c7cc;
}

.order-detail ul li:last-child {
  border: none;
}

.order-detail .bank-info p:last-child {
  position: absolute;
  width: 0.213333rem;
  height: 0.426667rem;
  right: 0.586667rem;
  top: 0.533333rem;
  margin-top: -0.213333rem;
  color: rgb(111, 168, 255);
}

[data-dpr="1"] .order-detail .bank-info p:last-child {
  font-size: 24px;
}

[data-dpr="2"] .order-detail .bank-info p:last-child {
  font-size: 48px;
}

[data-dpr="3"] .order-detail .bank-info p:last-child {
  font-size: 72px;
}

@media screen and (max-width:320px) {
  .order-detail ul li p:nth-child(1) {
    color: rgb(109, 109, 109);
    width: 30%;
    margin-right: 10%;
    text-align: right;
  }
}

@media screen and (min-width: 330px) and (max-width: 375px) {
  .order-detail ul li p:nth-child(1) {
    color: rgb(109, 109, 109);
    width: 27%;
    margin-right: 10%;
    text-align: right;
  }
}

@media screen and (min-width: 376px) {
  .order-detail ul li p:nth-child(1) {
    color: rgb(109, 109, 109);
    width: 23.5%;
    margin-right: 10%;
    text-align: right;
  }
}

.order-detail ul li p:nth-child(2) {
  color: rgb(51, 51, 51);
}

.order-detail ul li p:nth-child(2) a {
  color: rgb(51, 51, 51);
}

.order-detail ul li p.paymentMoney {
  color: rgb(255, 180, 0);
}