.bank_list {
	border: 1px solid rgb(204, 204, 204);
	border-right: 0px;
	/*no*/
	height: 130px;
	margin: 2.67% 2.67% 0 2.67%;
	position: relative;
	padding: 0;
	background: white;
	transition: 0.3s;
}
.mui-ios .bank_list {
	border: 0.5px solid rgb(204, 204, 204);
	border-right: 0px;
}


.bank_list_delete {
	opacity: 0;
}

.bank_list .bank_list_left {
	background-color: #F3742B;
	width: 50px;
	height: 100px;
	float: left;
}

.bank_list .bank_list_right {
	height: 100%;
}

.span_bottom {
	font-size: 15px;
	/*px*/
	letter-spacing: 1px;
	color: rgb(119, 119, 119);
	margin-top: 15px;
	height: 15px;
	line-height: 15px;
}

.bank_delete_div {
	position: absolute;
	right: 5px;
	display: none;
}

.bank_delete_div a {
	font-size: 40px;
	/*px*/
}

.mui-table-view-cell:after {
	height: 0px;
}

.mui-btn {
	width: 90px;
}

.mui-loader {
	position: absolute;
	top: 30%;
	width: 100%;
	height: 60%;
	color: #888;
	font-size: 14px;
	/*px*/
	text-align: center;
}

.account-default {
	top: 41px;
	width: 65px;
	height: 19px;
	position: absolute;
	right: 25px;
	font-size: 12px;
	/*px*/
	text-align: center;
	color: rgb(124, 163, 246);
	border: 1px solid rgb(124, 163, 246);
	/*no*/
	border-radius: 10px;
	line-height: 19px;
}

.mui-backdrop {
	top: 44px;
}

.bank-back {
	background: #0066ff;
}

.bank-back a,
.bank-back h1,
.bank-back span {
	color: #FFFFFF;
}

.bank-new {
	float: right;
	font-size: 14px;
	display: inline-block;
}

.bank-right-container,
.bank-left-container {
	float: left;
}

.bank-right-container {
	width: 4px;
	height: 100%;
	float: right;
}

.bank-left-container {
	/*border-top: 1px solid rgb(204, 204, 204);
	border-left: 1px solid rgb(204, 204, 204);
	border-bottom: 1px solid rgb(204, 204, 204);*/
	width: 95%;
	height: 100%;
	padding: 20px 0px;
	padding-left: 7.12%;
}

.mui-android .bank-blue {
	border-right: 2px solid rgb(111, 168, 255);
	/*background: rgb(111, 168, 255);*/
}

.mui-android .bank-gray {
	/*background: rgb(204, 204, 204);*/
	border-right: 2px solid rgb(204, 204, 204);
}
.mui-ios .bank-blue {
	border-right: 2px solid rgb(111, 168, 255);
	/*background: rgb(111, 168, 255);*/
}
.mui-ios .bank-gray {
	/*background: rgb(204, 204, 204);*/
	border-right: 2px solid rgb(204, 204, 204);
}

.bank-name {
	font-size: 18px;
	/*px*/
	color: rgb(51, 51, 51);
	height: 18px;
	line-height: 18px;
	margin-bottom: 5px !important;
}

.bank-subname {
	font-size: 14px;
	height: 30px;
	line-height: 14px;
	width: 67%;
}

.bank-delete {
	font-size: 16px;
	width: 100px;
	background: rgb(111, 168, 255);
	display: inline-block;
	line-height: 116px;
	text-align: center;
	padding-left: 21px !important;
}

.bank_list:last-child {
	margin-bottom: 10px;
}