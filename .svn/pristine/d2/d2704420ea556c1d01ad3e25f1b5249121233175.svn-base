		<script type="text/javascript" charset="UTF-8">
			mui.init();
			var time = dataFormat();
			var htTime = document.querySelector(".htTime");
			htTime.innerHTML = time;

			function dataFormat() {
				var time = new Date();
				var month = 0;
				var day = 0;
				var hours = 0;
				var minutes = 0;
				if(time.getMonth() < 9) {
					month = "0" + (time.getMonth() + 1);
				} else {
					month = time.getMonth() + 1;
				}
				if(time.getDate() < 9) {
					day = "0" + time.getDate();
				} else {
					day = time.getDate();
				}
				if(time.getHours() < 9) {
					hours = "0" + time.getHours();
				} else {
					hours = time.getHours();
				}
				if(time.getMinutes() < 9) {
					minutes = "0" + time.getMinutes();
				} else {
					minutes = time.getMinutes();
				}
				return time.getFullYear() + "-" + month + "-" + day + " " + hours + ":" + minutes;
			}
		</script>
