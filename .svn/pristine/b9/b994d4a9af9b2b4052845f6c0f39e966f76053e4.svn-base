.filter-pj ul {
	padding-left: 4%;
	padding-top: 10px;
	width: 100%;
	margin: 0px;
}

.filter-pj ul li {
	margin-bottom: 4%;
	margin-right: 4%;
	width: 29%;
	height: 45px;
	line-height: 45px;
	
	display: inline-block;
	/*font-size: 12px;*/
	/*px*/
	background-color: #fff;
	/*color: #666;*/
	text-align: center;
}
.mui-ios .filter-pj ul li{
	border: solid 0.5px #ccc;
}
.mui-android .filter-pj ul li{
	border: solid 1px #ccc;
}

.filter-pj p {
	margin-bottom: 0px;
	padding-left: 4%;
}

.filter-pj ul li.active {
	color: #ffffff;
	background: #0066ff;
}

.filter-pj .btn-wraper {
	margin-top: 18%;
	width: 100%;
}

@media only screen and (max-width:380px) {
	.filter-pj .btn-wraper {
		margin-top: 12%;
	}
}

.btn-wraper button {
	width: 50%;
	border-right-width: 0px;
	height: 50px;
	padding: 0px;
	border-radius: 0;
	font-size: 16px;
}


