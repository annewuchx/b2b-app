html,
body {
	height: 100%;
}

.mui-loader {
	position: absolute;
	top: 30%;
	width: 100%;
	height: 60%;
	color: #888;
	font-size: 14px;
	/*px*/
	text-align: center;
}

.mui-backdrop {
	top: 44px;
}

.bank-back {
	background: #0066ff;
}

.bank-back a,
.bank-back h1,
.bank-back span {
	color: #FFFFFF;
}

.userpj-pro {
	float: right;
	font-size: 14px;
	/*px*/
	display: inline-block;
}

.mui-input-group .mui-input-row {
	font-size: 14px;
	/*px*/
	height: 45px;
}

.mui-input-group .mui-input-row label {
	line-height: 45px;
	padding: 0px 15px;
	width: 28%;
}

.bill-amount {
	width: 72%;
	line-height: 45px;
	display: inline-block;
	padding-left: 15px;
	float: left;
}

.code-day-row {
	height: 25px;
	line-height: 25px;
	background: #6fa8ff;
	color: #FFFFFF;
	position: relative;
	font-size: 12px;
	/*px*/
}

.bar {
	position: absolute;
	width: 1px;
	height: 15px;
	display: inline-block;
	background: #FFFFFF;
	right: 36%;
	top: 5px;
}

.bill-left {
	width: 64%;
	padding-left: 10px
}

.bill-right {
	width: 36%;
	text-align: left;
	padding-left: 20px;
}

.right-next {
	color: #6fa8ff;
	display: inline-block;
	position: absolute;
	right: 0px;
	top: 20px;
}

.bill-detail {
	padding: 10px 0;
	height: 100%;
	text-align: center;
/*	border-bottom: 1px solid #CCCCCC;*/
	/*no*/
	position: relative;
}




.mui-table-view:after {
	height: 0px;
}

.mui-card {
	box-shadow: none;
	border-radius: 0px;
}

.border-container {
	position: relative;
}
.mui-ios .border-container{
	border-left: 0.5px solid #c8c7cc;
	border-right: 0.5px solid #c8c7cc;
}
.mui-android .border-container{
	border-left: 1px solid #c8c7cc;
	border-right: 1px solid #c8c7cc;
}

.mui-table-view-cell {
/*	position: relative;*/
/*	border: 1px solid red;*/
}

.condition-filter p,
.condition-filter ul li {
	color: rgb(51, 51, 51);
	font-size: 16px;
}

.condition-title p {
	color: rgb(51, 51, 51);
	font-size: 16px;
}
.condition-filter p:first-child{
	margin-top: 15px;
}

.bill-detail:after,.filter-header:after,.border-container:after{
	position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    background-color: #c8c7cc;
}
