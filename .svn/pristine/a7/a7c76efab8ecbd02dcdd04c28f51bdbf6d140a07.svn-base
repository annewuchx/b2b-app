html,
body {
  background: #F2F2F2;
  height: 100%;
}

.mui-indexed-list-empty-alert {
  display: none;
}

.mui-indexed-list-item {
  display: inline-block;
  margin-right: 4%;
  margin-bottom: 0.4rem;
  height: 1.066667rem;
  width: 28%;
  background: #FFFFFF;
  text-align: center;
  line-height: 1.066667rem;
  text-overflow: ellipsis;
  overflow: hidden;
  border: 1px solid rgb(204, 204, 204);
}

[data-dpr="1"] .mui-indexed-list-item {
  font-size: 15px;
}

[data-dpr="2"] .mui-indexed-list-item {
  font-size: 30px;
}

[data-dpr="3"] .mui-indexed-list-item {
  font-size: 45px;
}

.mui-province-group,
.mui-indexed-list-search {
  margin-left: 4%;
}

[data-dpr="1"] .mui-province-group,
[data-dpr="1"] .mui-indexed-list-search {
  font-size: 15px;
}

[data-dpr="2"] .mui-province-group,
[data-dpr="2"] .mui-indexed-list-search {
  font-size: 30px;
}

[data-dpr="3"] .mui-province-group,
[data-dpr="3"] .mui-indexed-list-search {
  font-size: 45px;
}

.mui-indexed-list-group {
  height: 0.666667rem;
  margin-bottom: 0.266667rem;
}

.mui-location div {
  float: left;
}

.mui-location {
  height: 1.2rem;
  background: #FFFFFF;
  line-height: 1.2rem;
  margin-bottom: 0.4rem;
  border-top: 1px solid rgb(204, 204, 204);
  border-bottom: 1px solid rgb(204, 204, 204);
}

[data-dpr="1"] .mui-location {
  font-size: 15px;
}

[data-dpr="2"] .mui-location {
  font-size: 30px;
}

[data-dpr="3"] .mui-location {
  font-size: 45px;
}

@media only screen and (max-width: 320px) {
  [data-dpr="1"] .location-des {
    font-size: 12px;
  }

  [data-dpr="2"] .location-des {
    font-size: 24px;
  }

  [data-dpr="3"] .location-des {
    font-size: 36px;
  }
}

@media only screen and (min-width: 330px) and (max-width: 360px) {
  [data-dpr="1"] .location-des {
    font-size: 14px;
  }

  [data-dpr="2"] .location-des {
    font-size: 28px;
  }

  [data-dpr="3"] .location-des {
    font-size: 42px;
  }
}

.mui-bar-nav.mui-bar a.mui-icon {
  color: #000;
}

[data-dpr="1"] .mui-bar-nav.mui-bar a.mui-icon {
  font-size: 30px;
}

[data-dpr="2"] .mui-bar-nav.mui-bar a.mui-icon {
  font-size: 60px;
}

[data-dpr="3"] .mui-bar-nav.mui-bar a.mui-icon {
  font-size: 90px;
}