.body_div_head_bg {
	width: 100%;
	padding-top: 70px;
	text-align: center;
	background: url(../images/bg_quote.png);
	background-size: 100% 100%;
	height: 230px;
}

.mui-bar {
	background: rgba(255, 255, 255, 0);
	box-shadow: none;
}

.user-header h1,
.user-header a {
	color: #FFFFFF !important;
}
.user-header {
	height: 44px; /*px*/
	line-height:44px;/*px*/
}
.user-header h1,.mui-bar .mui-btn-link{
	font-size: 16px; /*px*/
	
}


.company-name {
	font-size: 20px !important;
	color: #FFFFFF;
	margin-bottom: 0px;
}

.verified-btn {
	height: 40px;
	width: 100%;
}

.total-bill-desp {
	color: #FFFFFF;
	font-size: 15px;
	/*px*/
}

.total-bill {
	color: #FFFFFF;
	font-size: 40px;
	/*px*/
	line-height: 40px;
	margin-bottom: 30px;
}

.total-bill span {
	font-size: 28px;
	/*px*/
}

.hline {
	box-sizing: border-box;
	line-height: 0;
	background-color: #cccccc;
	width: 100%;
	height: 1px;  /*no*/
}

.verified-btn button {
	font-size: 15px;
	/*px*/
	margin-bottom: 15px;
	margin-top: 10px;
	color: #FFFFFF;
	padding: 0px;
	width: 75px; 
}

.mui-table-view-cell span {
	margin-left: 8.7%;
}

.mui-table-view a {
	font-size: 16px;
	/*px*/
}

.manage-info span {
	color: rgb(118, 175, 27);
}

.mui-navigate-right:after,
.mui-push-right:after {
	color: #85aaf7;
	font-size: 18px;
	/*px*/
}

.account-user {
	width: 52%;
	text-overflow: ellipsis;
	overflow: hidden;
}

.body_div_content{
	margin-top: 10px;
}

.body_div_content ul li {
	height: 45px;
}

.userindex-font {
	font-size: 20px !important;
}

[v-cloak] {
	display: none
}

.mui-table-view .mui-table-view-cell:last-child:after {
	height: 0px;
}


/**
 * mui
 */

.mui-table-view-cell:after {
	background-color: #cccccc;
}

.mui-table-view:before {
	background-color: #cccccc;

}

.mui-table-view:after {
	background-color: #cccccc;
}

.mui-table-view-cell>a:not(.mui-btn) {
	position: relative;
	display: block;
	overflow: hidden;
	margin: -11px -15px;
	padding: inherit;
	white-space: nowrap;
	text-overflow: ellipsis;
	color: inherit;
}

.mui-table-view-cell {
	position: relative;
	overflow: hidden;
	padding: 11px 15px;
	-webkit-touch-callout: none;
}

.mui-btn,
button,
input[type=button],
input[type=reset],
input[type=submit] {
	border-radius: 3px;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;
	border-bottom-left-radius: 3px;
}


/**
 * yckfont
 */

.yckfont {
	font-family: "yckfont" !important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: 0.2px;
	-moz-osx-font-smoothing: grayscale;
}