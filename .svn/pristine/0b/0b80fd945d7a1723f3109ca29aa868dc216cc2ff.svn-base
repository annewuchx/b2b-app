html,
body {
  width: 100%;
  height: 100%;
  min-width: 320px;
}

img {
  display: block;
}

ul,
li {
  list-style: none;
}

ul.quote-center-area-sum {
  margin: 0;
}

/*头部*/

.mui-bar {
  background-color: rgb(0, 102, 255);
}

header input.h-right {
  float: right;
  background: rgb(0, 102, 255);
}

header .mui-title {
  color: #fff;
}

header .h-left {
  color: #fff;
}

[data-dpr="1"] header .h-left span {
  font-size: 16px;
}

[data-dpr="2"] header .h-left span {
  font-size: 32px;
}

[data-dpr="3"] header .h-left span {
  font-size: 48px;
}

.mui-bar .mui-btn-link {
  color: #5590EF;
}

header input[type=button].h-right-active {
  color: #fff;
}